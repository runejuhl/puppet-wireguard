{
  "name": "runejuhl-wireguard",
  "version": "0.2.0",
  "author": "Rune Juhl Jacobsen",
  "summary": "Install and manage WireGuard tunnels. Based on flokli-wireguard and changes by aequitas.",
  "license": "MIT",
  "source": "https://github.com/runejuhl/puppet-wireguard.git",
  "project_page": "https://github.com/runejuhl/puppet-wireguard",
  "issues_url": "https://github.com/runejuhl/puppet-wireguard/issues",
  "tags": [
    "network",
    "vpn",
    "wireguard",
    "security"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.1.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 4.0.0"
    },
    {
      "name": "voxpupuli/tea",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "data_provider": null,
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "18.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8.0",
        "9.0"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0"
    }
  ]
}
